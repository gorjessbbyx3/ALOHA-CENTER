# Database Configuration
DATABASE_URL="postgresql://username:password@host:port/database"

# Stripe API Keys
STRIPE_SECRET_KEY=sk_test_your_secret_key
VITE_STRIPE_PUBLIC_KEY=pk_test_your_public_key

# Calendly Integration
CALENDLY_API_KEY=your_calendly_api_key
CALENDLY_USER_URI=https://api.calendly.com/users/your_user_id
CALENDLY_ORG_URI=https://api.calendly.com/organizations/your_org_id

# AWS RDS Configuration (optional, only if using AWS RDS)
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_REGION=us-east-2
# DB_INSTANCE_IDENTIFIER=your_db_instance
# DB_NAME=your_db_name
# DB_SECRET_ARN=your_secret_arn
# DB_RESOURCE_ARN=your_resource_arn

# Node Environment
NODE_ENV=development